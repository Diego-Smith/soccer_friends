# --- !Ups

create table "user" (
"id" INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,
"username" VARCHAR NOT NULL,
"password" VARCHAR NOT NULL
);

insert into "user" ("username","password","id") VALUES ('diego@diego2.it','welcome2',123);
insert into "user" ("username","password") VALUES ('diego@diego.it','welcome1');
insert into "user" ("username","password","id") VALUES ('diego@diego3.it','welcome2',1);

# --- !Downs

drop table "user";

